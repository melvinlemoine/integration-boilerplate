{
  "name": "integration-boilerplate",
  "version": "1.0.1",
  "scripts": {
    "deploy": "npm run build-clean && npm run build-sass && npm run build-autoprefix && npm run build-cleancss",
    "build-autoprefix": "postcss --use autoprefixer --autoprefixer.browsers '> 5%' --output assets/css/styles.css assets/css/styles.css",
    "build-cleancss": "cleancss -o assets/css/styles.css assets/css/styles.css",
    "build-clean": "rimraf css",
    "build-sass": "sass sass/styles.sass assets/css/styles.css",
    "watch": "npm run build-sass -- --watch"
  },
  "author": "https://github.com/melvinlemoine",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "clean-css-cli": "^5.5.0",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "postcss": "^8.4.19"
  },
  "description": ""
}
